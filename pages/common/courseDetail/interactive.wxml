<scroll-view scroll-y class="scrollPage">
  
  <!-- 选项 -->
  <view class="select">
    <view class="card bg-img shadow-blur padding margin" style="background-image:url({{item.url}})" wx:for="{{card}}">{{item.msg}}</view>
  </view>
  
  <!-- 讨论区 -->
  <view class="cu-bar bg-white margin-top-sm">
    <view class="action">
      <text class="cuIcon-title text-blue"></text> 讨论区
    </view>
  </view>
  
</scroll-view>