
.container-new {
  height: 85vh;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.form {
  width: 100%;
  margin-top: 3rem;
}

.cu-btn {
  width: 8rem;
}

.cu-form-group {
  background-color: #F4F6F9
}

.cu-form-group .title {
  min-width: calc(5em + 30rpx);
  padding-right: 0;
}

.bottom {
  width: 100%;
  text-align: center;
  margin-top: 10rem;
}

.bottom button {
  width: 90%;
  margin-bottom: 2rem;
}
