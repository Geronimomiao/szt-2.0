@import "/static/css/animate.wxss";

.point-container {
  height: 100vh;
  position: relative;
  width:100%;
  min-height:100%;
  background-size:100% 100%;
  background-image: url("https://img.wsmpage.cn/szt2/bg/bg3.png")
}

.bottom {
  position: fixed;
  bottom: 0;
  height: 15vh;
  width: 100%;
  background-color: #f0f0f0;
  
}

.choose {
  margin-top: 10vh;
}

.hasChoose {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 6rem;
  text-align: center;
  flex-direction: column;
}

.hasChoose .h12 {
  height: 12rem;
  width: 12rem;
  background-size:100% 100%;
}

.hasChoose .f2 {
  font-size: 1.5rem;
}

.animated.delay-g1s {
  animation-delay: 1.5s;
}

.animated.delay-s1s {
  animation-delay: 0.5s;
}

.point {
  display: flex;
}

.point .item {
  text-align: center;
  flex:  1;
}

.selected {
  display: flex;
  height: 6rem;
}

.selected .item {
  flex: 1;
  background-size:100% 100%;
}

.select-1 {
  background-image: url("data:image/png;base64,")
}

.select-2 {
  background: url("https://img.wsmpage.cn/szt2/bg/cloud.png")
}

.select-3 {
  background: url("https://img.wsmpage.cn/szt2/bg/badCloud.png")
}

.select-4 {
  background: url("https://img.wsmpage.cn/szt2/bg/wind.png")
}

